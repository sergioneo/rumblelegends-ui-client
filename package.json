{
  "name": "angular4-webpack-starter",
  "version": "0.0.1",
  "description": "Rumble Legends",
  "main": "index.ts",
  "scripts": {
    "serve:local": "webpack-dev-server --config config/webpack.local.js --port 8097 --progress --colors",
    "build:dev": "npm run clean && webpack --config config/webpack.dev.js --progress",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js --progress",
    "build": "npm run build:dev",
    "start": "node server.js",
    "postinstall": "npm run build:prod",
    "start:local": "npm run serve:local",
    "clean": "rimraf dist/ lib",
    "test": "karma start config/karma.conf.js",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor ./config/protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "ci": "npm run lint && npm test && npm run e2e",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "tslint": "tslint"
  },
